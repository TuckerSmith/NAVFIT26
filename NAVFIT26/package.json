{
  "name": "navfit26",
  "version": "1.0.0",
  "description": "NAVFIT26 Master Launcher",
  "main": "electron2.1/main.js",
  "homepage": "https://github.com/TuckerSmith/NAVFIT26",
  "author": {
    "name": "Tucker Smith",
    "email": "tuckers7401@yahoo.com"
  },
  "license": "ISC",
  "scripts": {
    "install-all": "npm install && cd frontend2.1 && npm install && cd ../electron2.1 && npm install",
    "build-ui": "cd frontend2.1 && npm run build",
    "dist": "npm run build-ui && electron-builder --linux deb",
    "postinstall": "electron-builder install-app-deps",
    "start": "npm run build-ui && cd electron2.1 && npm start"
  },
  "dependencies": {
    "better-sqlite3": "^11.10.0"
  },
  "devDependencies": {
    "electron": "^25.9.8",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "com.navfit26.app",
    "productName": "NAVFIT26",
    "files": [
      "electron2.1/**/*",
      "frontend2.1/dist/**/*",
      "backend2.1/**/*",
      "package.json"
    ],
    "linux": {
      "target": ["deb"],
      "maintainer": "Tucker Smith <tuckers7401@yahoo.com>",
      "category": "Office"
    },
    "asar": true
  }
}